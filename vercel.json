{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "functions": {
    "app/api/youtube-transcript/route.ts": {
      "maxDuration": 30
    },
    "app/api/health/route.ts": {
      "maxDuration": 10
    },
    "app/api/proxy/route.ts": {
      "maxDuration": 20
    }
  },
  "env": {
    "NODE_ENV": "production",
    "FORCE_REBUILD": "2025-07-30"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}